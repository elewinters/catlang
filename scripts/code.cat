fn puts(a: i64);
fn printf(fmt: i64, n: i16);
fn putchar(c: i8);

fn myputchar(c: i8, c2: i8) {
	putchar(c);
	putchar(c2);
	putchar(10);
}

fn abc() {
	putchar(65);
	putchar(66);
	putchar(67);
	putchar(10); /* newline */
}

fn main() -> i32 {
	let bits16: i16 = 50;
	abc();

	asm!("xor rax, rax");
	printf("%hd\n", bits16);

	asm!("xor rax, rax");
	printf("%hd\n", bits16 + 1);

	return 0;
}
