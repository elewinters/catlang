fn meow() {
	syscall!(1, 1, "meow\n", 5);
}

fn print(str: int64, len: int64) {
	syscall!(1, 1, str, len);
}

fn _start() {
	let sys_write: int64 = 1;
	let stdout: int64 = 1;

	syscall!(sys_write, stdout, "Hello, world!\n", 14);
	
	let len: int64 = 6;
	print("hi :D\n", len);
	meow();

	let exit_code: int64 = 0;
	syscall!(60, exit_code);
}